   ],
   "source": [
    "n = int(input(\"Num : \"))\n",
    "\n",
    "divisor = 3\n",
    "\n",
    "while divisor < n:\n",
    "    remain = n % divisor\n",
    "    print(n, \"%\", divisor, \"=\", remain)\n",
    "\n",
    "    divisor += 2\n",
    "\n",
    "    if remain == 0:\n",
    "        print(n, \"is compsite number\")\n",
    "        break\n",
    "\n",
    "else:\n",
    "    print(n, \"is prime\")"
   ]
  }
